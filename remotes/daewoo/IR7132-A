          KEY_MUTE                 0x00000000000000D8        #  Was: Mute
          KEY_VOLUMEDOWN           0x0000000000000038        #  Was: Volume-
          KEY_DOWN                 0x00000000000000B8        #  Was: Down
          TV/VCR                   0x0000000000000078
          KEY_SLEEP                0x00000000000000F8        #  Was: Standby
          KEY_3-                   0x0000000000000014        #  Was: 3-
          KEY_4-                   0x0000000000000094        #  Was: 4-
          KEY_CHANNELUP            0x000000000000000C        #  Was: Channel+
          KEY_CHANNELDOWN          0x000000000000002C        #  Was: Channel-
          KEY_NEXT                 0x000000000000001C        #  Was: Skip
          System                   0x000000000000009C
